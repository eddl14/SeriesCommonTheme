{% extends 'base.html.twig' %}

{% block body %}
    <h1>Saison</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ saison.id }}</td>
            </tr>
            <tr>
                <th>Nomsaison</th>
                <td>{{ saison.nomSaison }}</td>
            </tr>
            <tr>
                <th>Numerosaison</th>
                <td>{{ saison.numeroSaison }}</td>
            </tr>
            <tr>
                <th>Resumesaison</th>
                <td>{{ saison.resumeSaison }}</td>
            </tr>
            <tr>
                <th>Anneeproduction</th>
                <td>{{ saison.anneeProduction }}</td>
            </tr>
            <tr>
                <th>Postersaison</th>
                <td>{{ saison.photo }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('saison_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('saison_edit', { 'id': saison.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
